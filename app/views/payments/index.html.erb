<h1>Inbetalningar</h1>

<table>
  <tr>
    <th>User</th>
    <th>User ID</th>
    <th>Paymill payment</th>
    <th>Paymill token</th>
    <th></th>
    <th></th>
  </tr>

<% @payments.each do |payment| %>
  <tr>
    <td><%= payment.user.name %></td>
    <td><%= payment.user_id %></td>
    <td><%= payment.paymill_payment_id %></td>
    <td><%= payment.paymill_token %></td>
    <td><%= link_to 'Show', payment %></td>
    <td><%= link_to 'Destroy', payment, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Payment', new_payment_path %>
